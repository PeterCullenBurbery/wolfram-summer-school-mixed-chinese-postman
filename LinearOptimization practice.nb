Cell[BoxData[
 RowBox[{
  RowBox[{"singleTourConstraint", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"i", "\[Equal]", "j"}], ",", 
        RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Indexed", "[", 
           RowBox[{"u", ",", "i"}], "]"}], "-", 
          RowBox[{"Indexed", "[", 
           RowBox[{"u", ",", "j"}], "]"}], "+", 
          RowBox[{"n", "*", 
           RowBox[{"Indexed", "[", 
            RowBox[{"x", ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "j"}], "}"}]}], "]"}]}]}], "\[LessEqual]", 
         RowBox[{"n", "-", "1"}]}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "2", ",", "n"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "2", ",", "n"}], "}"}]}], "]"}], "]"}]}], 
  ";"}]], "Input",
 CellLabel->"In[43]:=",
 CellID->269204625,ExpressionUUID->"e91f2e45-fa20-43c7-9966-8cec5aabe861"],

