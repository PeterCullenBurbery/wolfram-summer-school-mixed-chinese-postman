Cell[TextData[{
 "Find the path that a salesman should take through ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath",ExpressionUUID->
  "81aa0bdd-86af-423a-b622-92313f5d4032"],
 StyleBox[" cities such that each city is only visited once and that \
minimizes the distance. Generate the locations:",
  Deletable->True]
}], "ExampleText",
 CellID->1676868274,ExpressionUUID->"64586d33-83ee-431b-9476-99529a6f72c7"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"n", "=", "20"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pts", " ", "=", " ", 
   InterpretationBox[
    DynamicModuleBox[{Typeset`open = False}, 
     TemplateBox[{"Expression", 
       RowBox[{"CompoundExpression", "[", 
         DynamicBox[
          FEPrivate`FrontEndResource["FEBitmaps", "IconizeEllipsis"]], "]"}], 
       GridBox[{{
          RowBox[{
            TagBox["\"Byte count: \"", "IconizedLabel"], "\[InvisibleSpace]", 
            TagBox["264", "IconizedItem"]}]}}, 
        GridBoxAlignment -> {"Columns" -> {{Left}}}, DefaultBaseStyle -> 
        "Column", 
        GridBoxItemSize -> {
         "Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}}], 
       Dynamic[Typeset`open]},
      "IconizedObject"]],
    SeedRandom[123]; RandomReal[10, {$CellContext`n, 2}],
    SelectWithContents->True,
    Selectable->False]}], ";"}]}], "Input",
 CellChangeTimes->{3.8662068977768183`*^9, 3.8662069511748295`*^9},
 CellLabel->"In[37]:=",
